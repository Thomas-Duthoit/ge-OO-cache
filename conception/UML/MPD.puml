@startuml

object Utilisateur{
  {static} id : int
  + pseudo : String 
  + mdp : String
  + admin : bool
}

object ReseauCache{
  {static} id : int
  # <b>idProprietaire : int</b>
  + nom : String
}

object Utilisateur_reseau{
  # <b>idUtiliisateur : int</b>
  # <b>idReseau: int</b>
}

object Cache{
  {static} numero : int
  # <b>idReseau : int</b>
  + descriptionTextuelle : String
  + descriptionTechnique : String 
  + rubriqueLibre : String
  + type : TypeCache
  + statut : StatutCache
}

object Log{
  {static} id : int
  # <b>idCache : int</b>
  # <b>idUtilisateur : int</b>
  + commentaire : String
  + note : int
  + trouver : bool
  + date : DateTime
  + image : url /optionnel/
}

Utilisateur "0..*" -- "0..*" ReseauCache 
(Utilisateur, ReseauCache) . Utilisateur_reseau

Cache::idReseau "1" --- "0..*" ReseauCache::id 

Cache::numero "0..*" --- "1" Log::idCache
Utilisateur::id "0..*" -- "1" ReseauCache::idProprietaire
Log::idUtilisateur "1" - "0..*" Utilisateur::id

@enduml
